<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="udhtu.Controller"
          xmlns:fx="http://javafx.com/fxml"
          alignment="center"
          hgap="10"
          vgap="10">
    <padding>
        <Insets
                top="25"
                right="25"
                bottom="25"
                left="25"/>
    </padding>

    <Button
            GridPane.columnIndex="1"
            GridPane.rowIndex="1"
            text="Тест 1"
            onAction="#handleFirstTestButtonAction"/>

    <Button
            GridPane.columnIndex="1"
            GridPane.rowIndex="2"
            text="Teст 2"
            onAction="#handleSecondTestButtonAction"/>

    <Button
            GridPane.columnIndex="1"
            GridPane.rowIndex="3"
            text="Teст 3"
            onAction="#handleThirdTestButtonAction"/>

    <Button
            GridPane.columnIndex="1"
            GridPane.rowIndex="4"
            text="Teст 4"
            onAction="#handleFourthTestButtonAction"/>

    <Button
            GridPane.columnIndex="1"
            GridPane.rowIndex="5"
            text="Teст 5"
            onAction="#handleFifthTestButtonAction"/>

</GridPane>